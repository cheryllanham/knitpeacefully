<div class="row">
 <div class="col-md-6 col-md-offet-3">
  <% if params[:plan] == '2' %>
    <h1>Premium Account</h1>
    <p>Sign up ($10/mo.) for access to full site features, sell products, and receive discounts.</p>
  <% else %>
    <h1>Basic Account</h1>
    <p>Sign up for free to have basic access to our community.</p>
    <% end %>
  </div>
  <div class="col-md-6 col-md-offset-3">
    <div class="well">
      <h2>Sign up</h2>
      
      <% if params[:plan] == '2' %>
        <%= render "pro_form" %>
      <% else %>
        <%= render "basic_form" %>
      <% end %>
 
<%= render "devise/shared/links" %>
   
    </div>
  </div>
</div>
